{
	"format_version": "1.10.0",
	"animation_controllers": {
	"controller.animation.aiplayer.swimming.box" : {
			"initial_state" : "default",
			"states" : {
				"default" : {
				   "on_entry": [
						"/event entity @s no_swimming"	
					],
					"transitions" : [
						{
							"swimming" : "query.is_swimming"
						}
					]
				},
				"swimming" : {
					"on_entry": [
						"/event entity @s is_swimming"	
					],
					"transitions" : [
						{
							"default" : "!query.is_swimming"
						}
					]
				}
			}
		}
	}
}